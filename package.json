{
	"name": "@ff6347/p5-sandbox",
	"private": true,
	"version": "1.3.0",
	"type": "module",
	"scripts": {
		"copy:p5": "mkdirp ./public/lib && shx cp -r ./node_modules/p5/lib ./public/",
		"dev": "export PUBLIC_BASE_URL=$DEPLOY_URL && astro dev",
		"start": "export PUBLIC_BASE_URL=$DEPLOY_URL && astro dev",
		"prebuild": "npm run copy:p5",
		"postinstall": "npm run copy:p5",
		"build": "export PUBLIC_BASE_URL=$DEPLOY_URL && astro build",
		"check": "astro check",
		"preview": "astro preview",
		"astro": "astro",
		"test": "vitest",
		"test:e2e": "playwright test"
	},
	"dependencies": {
		"@astrojs/check": "0.3.1",
		"@astrojs/react": "3.0.4",
		"@monaco-editor/react": "4.6.0",
		"@types/p5": "1.7.3",
		"@types/react": "18.2.35",
		"@types/react-dom": "18.2.14",
		"astro": "3.5.2",
		"lodash.debounce": "4.0.8",
		"p5": "1.8.0",
		"prettier": "3.1.0",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"typescript": "5.3.2"
	},
	"devDependencies": {
		"@playwright/test": "1.39.0",
		"@technologiestiftung/semantic-release-config": "1.2.4",
		"@testing-library/react": "14.1.0",
		"@types/lodash.debounce": "4.0.9",
		"@types/node": "20.9.0",
		"jsdom": "22.1.0",
		"mkdirp": "3.0.1",
		"netlify-cli": "17.1.0",
		"shx": "0.3.4",
		"vitest": "0.34.6"
	}
}
